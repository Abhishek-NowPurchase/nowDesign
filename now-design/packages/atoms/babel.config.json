{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["inline-react-svg", {
      "svgo": {
        "plugins": [{
          "name": "removeAttrs",
          "params": {
            "attrs": "(width|height)"
          }
        }]
      }
    }]
  ]
} 