{
  "colorStyles": {
    "glassStroke": {
      "gradient": {
        "type": "linear-gradient",
        "stops": [
          { "position": 0, "color": "{brand.grey.400.value}" },
          { "position": 1, "color": "{brand.grey.100.value}" }
        ]
      }
    }
  },
  "effectStyles": {
    "backgroundBlurGlass": {
      "type": "background-blur",
      "color": { "value": "{alias.glass.white_0.value}" }
    },
    "dropShadow": {
      "type": "drop-shadow",
      "color": { "value": "{alias.glass.black_60.value}" },
      "positionX": { "value": "{effects.positionX.none.value}" },
      "positionY": { "value": "{effects.positionY.none.value}" },
      "blur": { "value": "{effects.blur.xl.value}" },
      "spread": { "value": "{effects.spread.none.value}" }
    },
    "layerBlur": {
      "type": "layer-blur",
      "color": { "value": "{alias.glass.black_0.value}" },
      "blur": { "value": "{effects.blur.xl.value}" }
    },
    "innerShadow": {
      "type": "inner-shadow",
      "color": { "value": "{alias.glass.black_20.value}" },
      "positionX": { "value": "{effects.positionX.none.value}" },
      "positionY": { "value": "{effects.positionY.none.value}" },
      "blur": { "value": "{effects.blur.m.value}" },
      "spread": { "value": "{effects.spread.none.value}" }
    }
  }
} 